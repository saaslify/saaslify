{
  "name": "saaslify",
  "description": "Saaslify SDK for JavaScript, Node and TypeScript",
  "version": "1.0.0",
  "repository": "git@github.com:saaslify/saaslify.git",
  "author": "saaslify.com",
  "browser": "dist/saaslify.min.js",
  "license": "Apache-2.0",
  "private": false,
  "scripts": {
    "dev": "node scripts/dev.js & yarn watch",
    "watch": "yarn rollup -c -w",
    "test": "jest",
    "lint": "prettier --write src/*",
    "build": "rimraf dist && yarn rollup -c",
    "prepublish": "yarn build",
    "create-release": "semantic-release",
    "format:check": "prettier --list-different \"src/**/*.ts\"",
    "test:ci": "CI=true yarn test --maxWorkers=2"
  },
  "dependencies": {
    "jsonwebtoken": "^8.5.1"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.9.6",
    "@commitlint/cli": "^9.1.2",
    "@commitlint/config-conventional": "^9.1.2",
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-node-resolve": "^7.1.3",
    "@rollup/plugin-replace": "^2.3.2",
    "@types/jest": "^25.2.2",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/node": "^14.0.1",
    "core-js": "^3.6.5",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "jsdom": "^16.2.2",
    "live-server": "^1.2.1",
    "markdown-it": "^11.0.0",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5",
    "rollup": "^2.3.1",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-postcss": "^3.1.0",
    "rollup-plugin-scss": "^2.1.0",
    "rollup-plugin-terser": "^7.0.1",
    "rollup-plugin-typescript2": "^0.27.0",
    "semantic-release": "^17.1.1",
    "ts-jest": "^26.0.0",
    "typescript": "^3.9.2"
  },
  "browserslist": {
    "production": [
      ">0.25%",
      "not dead"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
